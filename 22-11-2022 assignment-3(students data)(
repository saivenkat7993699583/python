sno=int(input("Enter the student roll number : "))
name=input("Enter the student name : ")
gender=input("Enter student gender : ")
group=input("Enter the student group : ")
address=input("Enter the student address : ")
phno=int(input("Enter the student phone number : "))
print("Student number = ",sno)
print("Name of the student = ",name)
print("Gender of the student = ",gender)
print("Group of the student = ",group)
print("Address of the student = ",address)
print("Phone numeber of the student = ",phno)
